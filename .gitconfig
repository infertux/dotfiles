[user]
	name = Cédric Félizard
	email = cedric@felizard.fr
	signingkey = 0xEEC73D5809A98A9B
[alias]
	staged = diff --cached
	unstage = reset HEAD --
	ignored = ls-files --others
	oneline = log --oneline --decorate
	last = log --oneline -1 HEAD
	out = log --oneline origin/master..HEAD
	wdiff = diff --color-words
	info = remote -v
	up = !git stash && git pull && git stash pop
	ll = log --pretty=oneline --abbrev-commit --max-count=15
	lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
	lp = log -p
	thanks = shortlog -sne
	diff-empty = diff 4b825dc642cb6eb9a060e54bf8d69288fbee4904
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	delete-merged = !git branch --merged master | grep -Ev '(dev|development|master|main|production)$' | xargs git branch -d
	unfuck = reset --soft HEAD@{1}
[color]
	ui = auto
[merge]
	tool = vimdiff
	conflictstyle = diff3
[diff]
	renames = copies
	mnemonicprefix = true
	submodule = log
[branch]
	autosetuprebase = always
[color "diff"]
	whitespace = yellow reverse
[push]
	default = tracking
[core]
	excludesfile = /home/infertux/gitignore
[log]
	date = iso
[mergetool]
	keepBackup = false
[rerere]
	enabled = 1
